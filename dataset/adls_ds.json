{
	"name": "adls_ds",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage_LS",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"container": {
				"type": "string"
			},
			"layer": {
				"type": "string"
			},
			"city": {
				"type": "string"
			},
			"fileName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(dataset().city, '/', dataset().fileName)",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().layer",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().container",
					"type": "Expression"
				}
			},
			"columnDelimiter": "\t",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}